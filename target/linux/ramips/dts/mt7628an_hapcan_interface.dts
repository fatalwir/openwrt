#include "mt7628an_hilink_hlk-7628n.dts"

&{/} {
	compatible = "hapcan,interface";
	model = "HAPCAN Ethernet Interface";
};

&state_default {
	// Override GPIO assignment from the mt7628an_hilink_hlk-7628n.dts
	/delete-node/ gpio;

	// I2C assignment is done by the i2c-mt7621 kernel module at boot
};

&i2c {
	status = "okay";

	rtc@68 {
		compatible = "ti,bq32000";
		reg = <0x68>;
	};

	adc@6e {
	    compatible = "microchip,mcp3422";
	    reg = <0x6E>;
	};

};
